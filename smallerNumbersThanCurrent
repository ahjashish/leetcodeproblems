def smallerNumbersThanCurrent(nums):
       result = []
       for i in range(0,len(nums)):
           count = 0
           for j in range(0,len(nums)):
               if nums[j] != nums[i] and nums[j] < nums[i]:
                    count+=1
           result.append(count)
                    
       return result
       
        
print(smallerNumbersThanCurrent([8,1,2,2,3]))
print(smallerNumbersThanCurrent([7,7,7,7]))

# Create list to store results and count
# to count how many numbers are smaller
# than current number,then make comparation
# of indexes which start  0
# and increment count,and in the end append
# to the list.
